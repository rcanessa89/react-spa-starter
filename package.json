{
  "name": "react-spa-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "6.0.1",
    "history": "^4.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2"
  },
  "scripts": {
    "start": "concurrently \"node-sass-chokidar --include-path src/scss src/ !src/scss -o src/ --watch --recursive --output-style compressed\" \"react-scripts-ts start\"",
    "build": "node-sass-chokidar --include-path src/scss src/ -o src/ --output-style compressed && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-loadable": "^5.4.0",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux-logger": "^3.0.6",
    "concurrently": "^3.6.1",
    "node-sass-chokidar": "^1.3.3",
    "redux-logger": "^3.0.6",
    "typescript": "^2.9.2",
    "why-did-you-update": "^0.1.1"
  }
}
